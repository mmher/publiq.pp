module CommanderMessage
{
    class StringValue
    {
        String value
    }
    class NumberValue
    {
        UInt64 value
    }

    class NumberPair
    {
        UInt64 first
        UInt64 second
    }

    class Coin
    {
        UInt64 whole
        UInt64 fraction
    }

    class Account
    {
        String address
        Coin balance
    }

    class AccountResponse
    {
        String address
        Coin balance
        Coin confirmed_balance
        Coin unconfirmed_sent
        Coin unconfirmed_received
    }

    class Done
    {
    }

    class Failed
    {
        String message
        Extension reason
    }

    class HeadBlockRequest
    {}

    class AccountRequest
    {
        String address
    }

    class AccountsRequest
    {}

    class AccountsResponse
    {
        Array AccountResponse accounts
    }

    class AccountHistoryRequest
    {
        String address
        UInt64 start_block_index
        UInt64 max_block_count
    }

    class AccountHistory
    {
        Array AccountHistoryItem log
    }

    enum AccountHistoryItemType
    {
        received
        received_fee
        sent
        sent_fee
        rewarded
    }
    class AccountHistoryItem
    {
        UInt64 block_index
        UInt64 confirmations
        AccountHistoryItemType item_type
        TimePoint timestamp
        Coin amount

        Object details
    }
    class AccountHistoryReceived
    {
        String from
        String message
        String transaction_hash
    }
    class AccountHistoryReceivedFee
    {
        String from
        String transaction_hash
    }
    class AccountHistorySent
    {
        String to
        String message
        String transaction_hash
    }
    class AccountHistorySentFee
    {
        String to
        String transaction_hash
    }
    class AccountHistoryRewarded
    {
    }

    class ImportAccount
    {
        String address
    }

    class BlockInfoRequest
    {
        UInt64 block_number
    }

    class BlockInfo
    {
        String authority
        String block_hash
        UInt64 block_number
        UInt64 block_size
        TimePoint time_signed
    }

    class Send
    {
        String private_key
        String to
        Coin amount
        Coin fee
        String message
        UInt64 seconds_to_expire
    }

    class MinersRequest
    {
        UInt64 start_block_index
        UInt64 end_block_index
    }

    class MinersResponse
    {
        Array MinersResponseItem miners
    }

    class MinersResponseItem
    {
        String miner_address
        Array UInt64 block_numbers
    }

    class FilesRequest
    {
        RequestMode request_mode
    }

    enum RequestMode
    {
        channel
        storage
    }

    class FilesResponse
    {
        Array FilesResponseItem items
    }

    class FilesResponseItem
    {
        String address
        Array FlagedFile files_uri
    }

    enum Flag
    {
        on
        off
    }

    class FlagedFile
    {
        Flag flag
        String file_uri
    }

}
